{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/images/*",
      "/favicon.ico",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.css",
      "/*.js"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}


